{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/vite"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ]
}
